<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tsdata.sys.entity.NewsExt" table="news">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="native" />
        </id>
		<property name="title" column="title" type="java.lang.String" />
		<property name="title_en" column="title_en" type="java.lang.String" />
		<property name="postdate" column="postdate" type="java.lang.String" />
		<property name="smallimg" column="smallImg" type="java.lang.String" />
		<property name="bigimg" column="bigImg" type="java.lang.String" />
		<property name="content" column="content" type="java.lang.String" />
		<property name="content_en" column="content_en" type="java.lang.String" />
    </class>
</hibernate-mapping>