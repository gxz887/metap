<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.tsdata.sys.entity.MenuExt" table="menu">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="native" />
        </id>
		<property name="name" column="name" type="java.lang.String" />
		<property name="name_en" column="name_en" type="java.lang.String" />
		<property name="url" column="url" type="java.lang.String" />
		<property name="pid" column="pid" type="java.lang.String" />
    </class>
</hibernate-mapping>